<?xml version="1.0" encoding="UTF-8"?>
<!-- 
templateType=READ|WRITE 表示解析文件还是生成文件 
split分隔符 "\|" , 读文件时竖线分隔需要带转换符，写文件时不需要
headLength文件头的长度，带换行符的表示文件头占了多少行，不带换行符的应该是字节长度
endingLength文件尾
lineSeparator换行符，若无，则表示不换行，换行一般是\r\n 字节文件一般是固定长度不带分隔符不换行
-->
<data templateType="READ"
	split="\," 
	clazz="com.upay.batch.stepservice.chk.wechat.bean.ChkWechatBean"
	charSet="UTF-8"
	headLength="1"
	endingLength="2"
	lineSeparator="\r\n"
	>
<!-- 
regex正则表达式 
dateFormat日期格式
numMultiple数字类型需要转换到倍数
tranSrc要转换到源值，逗号分隔
tranDest要转换到目标值，逗号分隔
-->	

	<column name="txnTime"  length=""   regex=""    dateFormat=""   numMultiple=""  desc="交易时间" tranSrc=""  tranDest=""/>
	<column name="appid"  length=""   regex=""    dateFormat=""   numMultiple=""  desc="公众账号ID" tranSrc=""  tranDest=""/>
	<column name="wcMerNo"  length=""   regex=""    dateFormat=""   numMultiple=""  desc="微信商户号" tranSrc=""  tranDest=""/>
