	<column name="traceNo"  length=""   regex=""    dateFormat=""   numMultiple=""  desc="设备号" tranSrc=""  tranDest=""/>
	<column name="wcOrderNo"  length=""   regex=""    dateFormat=""   numMultiple=""  desc="微信订单号" tranSrc=""  tranDest=""/>
	<column name="orderNo"  length=""   regex=""    dateFormat=""   numMultiple=""  desc="平台订单号商户订单号" tranSrc=""  tranDest=""/>
	<column name="userFlag"  length=""   regex=""    dateFormat=""   numMultiple=""  desc="用户标识" tranSrc=""  tranDest=""/>
	<column name="txnType"  length=""   regex=""    dateFormat=""   numMultiple=""  desc="交易类型" tranSrc=""  tranDest=""/>
	<column name="txnStat"  length=""   regex=""    dateFormat=""   numMultiple=""  desc="交易状态" tranSrc=""  tranDest=""/>
	<column name="payBankNo"  length=""   regex=""    dateFormat=""   numMultiple=""  desc="付款银行" tranSrc=""  tranDest=""/>
	<column name="ccy"  length=""   regex=""    dateFormat=""   numMultiple=""  desc="币种" tranSrc=""  tranDest=""/>
	<column name="txnAmt"  length=""   regex=""    dateFormat=""   numMultiple=""  desc="交易总金额" tranSrc=""  tranDest=""/>
	<column name="voucherAmt"  length=""   regex=""    dateFormat=""   numMultiple=""  desc="企业红包金额" tranSrc=""  tranDest=""/>
	<column name="wcRefundOrderNo"  length=""   regex=""    dateFormat=""   numMultiple=""  desc="微信退款单号" tranSrc=""  tranDest=""/>
	<column name="refundOrderNo"  length=""   regex=""    dateFormat=""   numMultiple=""  desc="商户退款单号" tranSrc=""  tranDest=""/>
	<column name="refundAmt"  length=""   regex=""    dateFormat=""   numMultiple=""  desc="退款金额" tranSrc=""  tranDest=""/>
	<column name="refundVoucherAmt"  length=""   regex=""    dateFormat=""   numMultiple=""  desc="企业红包退款金额" tranSrc=""  tranDest=""/>
	<column name="refundType"  length=""   regex=""    dateFormat=""   numMultiple=""  desc="退款类型" tranSrc=""  tranDest=""/>
	<column name="refundStat"  length=""   regex=""    dateFormat=""   numMultiple=""  desc="退款状态" tranSrc=""  tranDest=""/>
	<column name="commodityName"  length=""   regex=""    dateFormat=""   numMultiple=""  desc="商品名称" tranSrc=""  tranDest=""/>
