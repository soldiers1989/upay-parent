<?xml version="1.0" encoding="UTF-8"?>

<!-- 支付宝订单撤销 -->
<data templateType="OUT"
	transCode="cancel">
<!-- 
oldName转换前参数名
newName转换后参数名
type转换类型
must是否必输
dateFormat日期格式
-->	
	<!-- 商户的订单号 -->
	<column oldName="out_trade_no" newName="outTradeNo"/>
	<!-- 支付宝交易号 -->
	<column oldName="trade_no" newName="tradeNo"/>
	<!-- 是否需要重试 -->
	<column oldName="retry_flag" newName="retryFlag"/>
	<!-- 本次撤销触发的交易动作 close：关闭交易，无退款  refund：产生了退款 -->
	<column oldName="action" newName="action"/>
</data>