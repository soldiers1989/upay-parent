<?xml version="1.0" encoding="UTF-8"?>
<!-- 
templateType=READ|WRITE 表示转换类型：交易前转换;交易后转换 
transCode需要转换参数的交易，此值同种转换类型不可重复
-->
<!-- 
银联交易状态查询
 -->
<data templateType="IN"
    transCode="00"
    >
<!-- 
oldName转换前参数名
newName转换后参数名
type转换类型
must是否必输
dateFormat日期格式
-->	
	<column oldName="tranCode"	must="true" newName="txnType" type="" dateFormat="" />
	<column oldName="txnSubType" must="true" newName="txnSubType" type="" dateFormat="" />
	<column oldName="bizType" must="true" newName="bizType" type="" dateFormat="" />

	<column oldName="txnTime" must="true" newName="txnTime" type="" dateFormat="" />
	<column oldName="orderId" must="true" newName="orderId" type="" dateFormat="" />

	<column oldName="accessType" must="true" newName="accessType" type="" dateFormat="" />
	<column oldName="channelType"	must="false" newName="channelType" type="" dateFormat="" />
	<column oldName="acqInsCode" newName="acqInsCode" type="" must="false" dateFormat="" />
	<column oldName="merCatCode" newName="merCatCode" type="" must="false" dateFormat="" />
	<column oldName="merName" newName="merName" type="" must="false" dateFormat="" />
	<column oldName="merAbbr" newName="merAbbr" type="" must="false" dateFormat="" />
</data>
	
