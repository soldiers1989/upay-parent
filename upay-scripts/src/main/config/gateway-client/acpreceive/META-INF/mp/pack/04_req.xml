<?xml version="1.0" encoding="UTF-8"?>
<!-- 
templateType=READ|WRITE 表示转换类型：交易前转换;交易后转换 
transCode需要转换参数的交易，此值同种转换类型不可重复
-->
<!-- 
银联退货类交易
 -->
<data templateType="IN"
    transCode="04"
    >
<!-- 
oldName转换前参数名
newName转换后参数名
type转换类型
must是否必输
dateFormat日期格式
-->	

	<column oldName="tranCode" newName="txnType" type="" must="true" dateFormat="" />
	<column oldName="txnSubType" newName="txnSubType" type="" must="true" dateFormat="" />
	<column oldName="bizType" newName="bizType" type="" must="true" dateFormat="" />
	<column oldName="channelType" newName="channelType" type="" must="true" dateFormat="" />
	<column oldName="backUrl" newName="backUrl" type="" must="true" dateFormat="" />

	<column oldName="orderId" newName="orderId" type="" must="true" dateFormat="" />
	<column oldName="txnAmt" newName="txnAmt" type="" must="true" dateFormat="" />
	<column oldName="txnTime" newName="txnTime" type="" must="true" dateFormat="" />
	<column oldName="reqReserved" newName="reqReserved" type="" must="false" dateFormat="" />
	<column oldName="origQryId" newName="origQryId" type="" must="true" dateFormat="" />
	<column oldName="accessType" newName="accessType" type="" must="true" dateFormat="" />
	<column oldName="acqInsCode" newName="acqInsCode" type="" must="false" dateFormat="" />
	<column oldName="merCatCode" newName="merCatCode" type="" must="false" dateFormat="" />
	<column oldName="merName" newName="merName" type="" must="false" dateFormat="" />
	<column oldName="merAbbr" newName="merAbbr" type="" must="false" dateFormat="" />
</data>