<?xml version="1.0" encoding="UTF-8"?>
<!-- 
templateType=READ|WRITE 表示转换类型：交易前转换;交易后转换 
transCode需要转换参数的交易，此值同种转换类型不可重复
-->
<!-- 
银联无跳转支付授权查询
 -->
<data templateType="OUT"
	transCode="78"
	>
<!-- 
oldName转换前参数名
newName转换后参数名
type转换类型
must是否必输
dateFormat日期格式
-->	

	<column oldName="respCode" newName="respCode" type="" must="true" dateFormat="" />
	<column oldName="respMsg" newName="respMsg" type="" must="true" dateFormat="" />
    <column oldName="activateStatus" newName="activateStatus" type="" must="false" dateFormat="" />
	<column oldName="signature" newName="signature" type="" must="true" dateFormat="" />
	<column oldName="signMethod" newName="signMethod" type="" must="true" dateFormat="" />
	<column oldName="tokenPayData" newName="tokenPayDataQuery" type="" must="false" dateFormat="" />
    <column oldName="reqReserved" newName="reqReserved" type="" must="false" dateFormat="" />	
	<column oldName="payCardType" newName="payCardType" type="" must="false" dateFormat="" />	
</data>