<?xml version="1.0" encoding="UTF-8"?>
<!-- 
templateType=READ|WRITE 表示转换类型：交易前转换;交易后转换 
transCode需要转换参数的交易，此值同种转换类型不可重复
-->
<!-- 
下属商户查询 
 -->
<data templateType="OUT"
	transCode="SUBMCHQRY"
	>
<!-- 
oldName转换前参数名
newName转换后参数名
type转换类型
must是否必输
dateFormat日期格式
-->	

    <column oldName="return_code"	must="true" newName="returnCode" type="" dateFormat="" />
	<column oldName="return_msg" newName="returnMsg" type="" must="true" dateFormat="" />
	<column oldName="err_code"	must="false" newName="errCode" type="" dateFormat="" />
	<column oldName="err_code_des" newName="errCodeDes" type="" must="false" dateFormat="" />
    <column oldName="appid" newName="repAppid" type="" must="false" dateFormat="" />
    <column oldName="total" newName="total" type="" must="false" dateFormat="" />
    <column oldName="mchinfo" newName="mchinfo" type="" must="false" dateFormat=""/>
    <column oldName="result_code" newName="resultCode" type="" must="false" dateFormat="" />
</data>