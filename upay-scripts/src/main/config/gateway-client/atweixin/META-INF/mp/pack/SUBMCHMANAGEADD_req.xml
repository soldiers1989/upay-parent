<?xml version="1.0" encoding="UTF-8"?>
<!-- 
templateType=READ|WRITE 表示转换类型：交易前转换;交易后转换 
transCode需要转换参数的交易，此值同种转换类型不可重复
-->
<!-- 
下属商户录入 
 -->
<data templateType="IN"
    transCode="SUBMCHMANAGEADD"
    >
<!-- 
oldName转换前参数名
newName转换后参数名
type转换类型
must是否必输
dateFormat日期格式
-->	
	<column oldName="transCode" newName="transCode" type="" must="true" dateFormat="" />
	<column oldName="appid" newName="appid" type="" must="true" dateFormat="" />
	<column oldName="sub_mch_id" newName="sub_mch_id" type="" must="false" dateFormat="" />
    <column oldName="sign" newName="sign" type="" must="true" dateFormat="" />
	<column oldName="mch_id" newName="mch_id" type="" must="true" dateFormat="" />
  	<column oldName="merchant_name" newName="merchant_name" type="" must="true" dateFormat="" />
    <column oldName="merchant_shortname" newName="merchant_shortname" type="" must="true" dateFormat="" />
    <column oldName="service_phone" newName="service_phone" type="" must="true" dateFormat="" />
    <column oldName="contact" newName="contact" type="" must="false" dateFormat="" />
    <column oldName="contact_phone" newName="contact_phone" type="" must="false" dateFormat="" /> 
    <column oldName="contact_email" newName="contact_email" type="" must="false" dateFormat="" />  
    <column oldName="channel_id" newName="channel_id" type="" must="true" dateFormat="" />
    <column oldName="business" newName="business" type="" must="true" dateFormat="" />
    <column oldName="contact_wechatid_type" newName="contact_wechatid_type" type="" must="false" dateFormat="" />
    <column oldName="contact_wechatid" newName="contact_wechatid" type="" must="false" dateFormat="" />
    <column oldName="merchant_remark" newName="merchant_remark" type="" must="false" dateFormat="" />
</data>