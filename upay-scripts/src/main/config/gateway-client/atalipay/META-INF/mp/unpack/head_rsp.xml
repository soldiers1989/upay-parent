<?xml version="1.0" encoding="UTF-8"?>

<!-- 支付宝订单预创建 -->
<data templateType="OUT"
	transCode="head"
	>
<!-- 
oldName转换前参数名
newName转换后参数名
type转换类型
must是否必输
dateFormat日期格式
-->	
	<!-- 网关返回码 -->
	<column oldName="code" newName="returnCode" />
	<!-- 网关返回码描述 -->
	<column oldName="msg" newName="returnMsg" />
	<!-- 业务返回码 -->
	<column oldName="sub_code" newName="outCode" />
	<!-- 业务返回码描述-->
	<column oldName="sub_msg" newName="outMsg" />
</data>