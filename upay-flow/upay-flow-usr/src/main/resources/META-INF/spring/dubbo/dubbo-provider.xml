        <!-- 用户是否设置支付密码查询 -->
       <dubbo:service ref="SI_USR0002" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0002"/>
       
        <!-- 安全退出 -->
       <dubbo:service ref="SI_USR0003" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0003"/>
       
       <!-- 用户密码修改 -->
       <dubbo:service ref="SI_USR0004" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0004"/>
       
        <!-- 用户登录密码重置 -->
       <dubbo:service ref="SI_USR0005" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0005"/>
       
        <!-- 用户登录-->
       <dubbo:service ref="SI_USR0006" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0006"/>
       
       
        <!-- 个人信息查询 -->
       <dubbo:service ref="SI_USR0007" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0007"/>
       
