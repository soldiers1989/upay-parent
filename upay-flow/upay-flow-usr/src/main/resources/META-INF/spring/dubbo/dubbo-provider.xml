       <dubbo:service ref="SI_USR0014" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0014"/>
       
        <!-- 用户首次设置支付密码-->
       <dubbo:service ref="SI_USR0015" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0015"/>
       
        <!-- 用户支付密码修改校验(知道原支付密码)-->
       <dubbo:service ref="SI_USR0016" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0016"/>
       
        <!-- 用户更改手机号(原手机可接收验证码) -->
       <dubbo:service ref="SI_USR0018" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0018"/>
       
       <!-- 用户更改手机号(原手机不可接收验证码) -->
       <dubbo:service ref="SI_USR0017" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0017"/>
       
       <!-- 绑卡修改支付密码校验 -->
       <dubbo:service ref="SI_USR0019" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0019"/>
       
       <!-- 交易密码修改(绑卡) -->
       <dubbo:service ref="SI_USR0020" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0020"/>
       
       <!-- 重置手机号申请结果查询 -->
       <dubbo:service ref="SI_USR0021" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0021"/>
       
       <!-- 个人信息查询（忘记登录密码） -->
       <dubbo:service ref="SI_USR0022" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0022"/>
       
       <!-- 企业注册，发送邮件 -->
       <dubbo:service ref="SI_USR0023" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0023"/>
       
       <!-- 验证签名，有效期 -->
       <dubbo:service ref="SI_USR0024" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0024"/>
       
       <!-- 企业登录密码重置，发送邮件 -->
       <dubbo:service ref="SI_USR0025" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0025"/>
       
       <!-- 验证签名，有效期(重置登录密码) -->
       <dubbo:service ref="SI_USR0026" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0026"/>
       
       <!-- 唯一标识检查 -->
       <dubbo:service ref="SI_USR0027" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0027"/>
       <dubbo:service ref="SI_USR0028" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0028"/>

       <!--根据用户邮箱或者手机号查询 用户信息-->
       <dubbo:service ref="SI_USR0029" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0029"/>

		<!--检查商户是否重复-->
       <dubbo:service ref="SI_USR0030" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0030"/>
       <!--根据用户邮箱或者手机号查询 用户信息-->
       <dubbo:service ref="SI_USR0031" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0031"/>
</beans>