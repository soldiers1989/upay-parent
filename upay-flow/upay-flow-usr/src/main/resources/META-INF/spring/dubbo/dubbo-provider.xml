       <dubbo:service ref="SI_USR0008" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0008"/>
       
       <!-- 用户支付密码修改 -->
       <dubbo:service ref="SI_USR0009" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0009"/>
       
       <!-- 用户支付密码重置 -->
       <dubbo:service ref="SI_USR0010" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0010"/>
       
       <!-- 用户实名认证 -->
       <dubbo:service ref="SI_USR0011" interface="com.pactera.dipper.core.IDipperHandler" group="SI_USR0011"/>  
       
