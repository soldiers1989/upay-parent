		<mapping xpath="transComments" field="transComments" must="not"/>
		<mapping xpath="payServicType" field="payServicType" must="not"/>
		<mapping xpath="tokenId" field="tokenId" must="not"/>
		<mapping xpath="authCode" field="authCode" must="not"/>
		<mapping xpath="detail" field="detail" must="not"/>
		<mapping xpath="subAppid" field="subAppid" must="not"/>
		<mapping xpath="deviceInfo" field="deviceInfo" must="not"/>
		<mapping xpath="attach" field="attach" must="not"/>
